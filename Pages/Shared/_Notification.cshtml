@if (TempData["success"] != null)
{
    <script type="text/javascript">
        toastr.options = {
            "closeButton": true,
            "progressBar": true
        }
        toastr.success('@TempData["success"]');
    </script>
}
@if (TempData["error"] != null)
{
    <script type="text/javascript">
        toastr.options = {
            "closeButton": true,
            "progressBar": true
        }
        toastr.error('@TempData["error"]');
    </script>
}